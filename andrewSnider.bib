@article{Deng:2006,
  author = {Deng, Wei and Cheeseman, James R. and Frisch, Michael J.},
  title = {Calculation of Nuclear Spin−Spin Coupling Constants of Molecules with First and Second Row Atoms in Study of Basis Set Dependence},
  journal = {Journal of Chemical Theory and Computation},
  year = {2006},
  volume = {2},
  number = {4},
  pages = {1028-1037},
  doi = {10.1021/ct600110u},
  annote = {Proposal for a rigourous method of augmenting standard basis sets to better suit the calculation of spin-spin coupling NMR constants. Most common basis sets are optmized for valence electrons, and treatment of core electrons required for accurate coupling constants is lacking. Article lists several basis sets that have been introduced purely for this reason, but computational limitations inhibit their widescale use. Instead, a specific method for modifying any existing basis set is introduced and shown to have good results both small and large basis sets.},
  keywords = {coupling constants, NMR, DFT, basis sets, Fermi contact },
}
