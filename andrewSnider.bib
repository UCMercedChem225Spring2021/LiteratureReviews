@article{Wheeler:2012,
  author = {Wheeler, Steven E.},
  title = {Homodesmotic reactions for thermochemistry},
  journal = {WIREs Computational Molecular Science},
  year = {2012}
  volume = {2},
  number = {2},
  pages = {204-220},
  doi = {https://doi.org/10.1002/wcms.72},
  annote = {Review article on the potential use of systemic error cancellation in order to obtain chemical accurracy for thermodynamic calculations. Discusses the original introduction of isodesmic bond-seperation reactions, as well as its extensions to homodesmotic (HD) and hyperdesmotic reactions. Takes note of extensive confusion and misuse of various methods and terminology for HD reactions, and puts forth an organized hierarchal structure HD reactions going forward. HD and its extensions are shown to be robust across several levels of DFT theory in reducing errors associated with correlation, basis set selection, and zero point vibrational energy. },
  keywords = {Isodesmotic, homodesmotic, thermodynamics, error cancellation, focal point analysis, complete basis set limit, hydrocarbons},
}

