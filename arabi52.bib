@article{Martin:2009a,
  author = {Martin, Jason and Baker, Jon and Pulay, Peter},
  title = {Comments on the molecular geometry of ferrocene: The dangers of using quantum chemistry programs as black boxes},
  journal = {Journal of Computational Chemistry},
  year = {2009}
  volume = {30},
  number = {6},
  pages = {881-883},
  doi = {10.1002/jcc.21106},
  annote = {This 'comment' essentially serves as a warning about lack of understanding of the quantum mechanics behind electronic structure software. It discusses a paper published by Zhang, et. al., in which the group mistakenly calculated an excited state for ferrocene using 6-31+G** basis set. Zhang and co. followed traditional safety-check protocols to ensure their solutions were correct, yet they still found an unrelaxed solution. Pulay warns this kind of issue occurs when true understanding of underlying theory of software is not understood. Martin, et. al. finds correct solution using diffuse basis sets.},
  keywords = {SCF;Hessian;Gaussian;diffuse basis functions},
}

@article{Almlof:1982,
  author = {Almlöf, J. and Faegri Jr., K. and Korsell, K.},
  title = {Principles for a direct SCF approach to LICAO–MOab-initio calculations},
  journal = {Journal of Computational Chemistry},
  year = {1982},
  volume = {3},
  number = {3},
  pages = {385--399},
  doi = {https://doi.org/10.1002/jcc.540030314},
  annote = {The actual LCAO-MO method was developed in 1951 by Roothan and Hall. When inmplemented into computations, it has (up until the point of this paper's publication) been possible to calculate electronic structure--with a potentially cost-prohibitive double integration step. To improve the bottle-neck caused by this double integral, a direct SCF approach was designed. This apprach improved integral evaluation time, SCF convergence accelearation, and implemented symmetry blocking.},
  keywords = {SCF;pseudopotential;open-shell systems;symmetry blocking},
}

