  doi = {10.1146/annurev-physchem-042018-052300},
  annote = {Discusses belief that theory's strenght is not to purely predict physical phenomena accurately. Rather, Davidson believes that the strength of theory is in understanding reasons guiding physical phenomena. For this reason, he discusses getting 'the right reason for the right reason.' For example, he bleieves using semi-empirical methods provides teh righ solution for the wrong reason, while some ab initio methods may be inaccurate and give the wrong answer for the right answer. He believes it is best to strive for both accuracy and theoretical understanding.},
  keywords = {CI;SCF;MELD;Excited states;clusters},
}

